class Student
    @@no_students = 0

    def initialize(name, id, course)
        @name = name
        @id = id
        @course = course
        @@no_students = @@no_students + 1
    end

    def display_Student()
        print("Name: " + @name.to_s + "\n" +
              "ID: " + @id.to_s + "\n" +
              "Course: " + @course.to_s + "\n")
    end

    def Student.no_student()
        return @@no_students
    end


end

